
<div class="main">
  <div class="container">
    <h1>Academic Rank  <i class="fas fa-user-graduate"></i></h1> 
    <p>Add universities to our rank.</p>
  </div>
</div>
<br>

  <div class="new_university container">
    
    <%= form_for(@university) do |f| %>
      <table class="table">
        <tr>
          <td class="text-center"><%= f.label :name %></td>
          <td class="text-left line-input"> <%= f.text_field :university_name %></td>
        </tr>
        <tr>
          <td class="text-center"><%= f.label :final_score %></td>
          <td class="text-left line-input"><%= f.text_field :final_score %></td>
        </tr>
      </table>   	
      <div class="actions">
        <%= f.submit "Create", class:"btn" %>
    	</div>
  	<%end%>
    
  </div>

